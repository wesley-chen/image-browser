{
  "name": "angular-tour-of-heroes-webpack",
  "version": "1.0.0",
  "description": "QuickStart package.json from the documentation, supplemented with testing support",
  "scripts": {
    "docker-build": "docker build -t ng2-quickstart .",
    "docker": "npm run docker-build && docker run -it --rm -p 3000:3000 -p 3001:3001 ng2-quickstart",
    "pree2e": "npm run webdriver:update",
    "e2e": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "webdriver:update": "webdriver-manager update",
    "start": "webpack-dev-server --inline --progress --port 8080",
    "test": "karma start",
    "test-once": "karma start --single-run",
    "build.prod": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "start.prod": "lite-server -c config/bs-config.prod.json",
    "start.desktop": "rimraf dist-dev && webpack --config config/webpack.desktop.dev.js --watch --progress",
    "build.desktop.prod": "rimraf dist && webpack --config config/webpack.desktop.prod.js --progress --profile --bail",
    "start.desktop.prod": "electron dist"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@angular/common": "~2.2.0",
    "@angular/compiler": "~2.2.0",
    "@angular/core": "~2.2.0",
    "@angular/forms": "~2.2.0",
    "@angular/http": "~2.2.0",
    "@angular/platform-browser": "~2.2.0",
    "@angular/platform-browser-dynamic": "~2.2.0",
    "@angular/router": "~3.2.0",
    "@angular/upgrade": "~2.2.0",
    "angular-in-memory-web-api": "~0.1.13",
    "core-js": "^2.4.1",
    "electron": "1.4.7",
    "reflect-metadata": "~0.1.8",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.26"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.34",
    "@types/electron": "^1.4.27",
    "@types/jasmine": "^2.5.36",
    "@types/node": "^6.0.46",
    "@types/selenium-webdriver": "^2.53.33",
    "angular2-router-loader": "^0.3.4",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^2.2.4",
    "concurrently": "^3.1.0",
    "css-loader": "^0.25.0",
    "electron-connect": "^0.6.0",
    "electron-packager": "8.2.0",
    "electron-webpack-plugin": "^1.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "jasmine-core": "~2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-htmlfile-reporter": "^0.3.4",
    "karma-jasmine": "^1.0.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "lite-server": "^2.2.2",
    "node-sass": "^3.13.0",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "protractor": "4.0.11",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "tslint": "^3.15.1",
    "typescript": "^2.0.10",
    "webdriver-manager": "10.2.8",
    "webpack": "^1.13.0",
