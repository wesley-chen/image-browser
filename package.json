  "scripts": {
    "docker-build": "docker build -t ng2-quickstart .",
    "docker": "npm run docker-build && docker run -it --rm -p 3000:3000 -p 3001:3001 ng2-quickstart",
    "pree2e": "npm run webdriver:update",
    "e2e": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
    "lint": "tslint ./app/**/*.ts -t verbose",
    "webdriver:update": "webdriver-manager update",
    "start": "webpack-dev-server --inline --progress --port 8080",
    "test": "karma start",
    "test-once": "karma start --single-run",
    "build.prod": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "start.prod": "lite-server -c config/bs-config.prod.json",
    "start.desktop": "rimraf dist-dev && webpack --config config/webpack.desktop.dev.js --watch --progress --profile --colors --display-error-details --display-cached",
    "build.desktop.prod": "rimraf dist && webpack --config config/webpack.desktop.prod.js --progress --profile --bail",
    "start.desktop.prod": "electron dist"
  },
